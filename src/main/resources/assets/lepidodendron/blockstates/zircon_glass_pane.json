{
  "multipart": [
    {   "apply": { "model": "lepidodendron:zircon_glass_pane_post" }},
    {   "when": { "north": true },
      "apply": { "model": "lepidodendron:zircon_glass_pane_side" }
    },
    {   "when": { "east": true },
      "apply": { "model": "lepidodendron:zircon_glass_pane_side", "y": 90 }
    },
    {   "when": { "south": true },
      "apply": { "model": "lepidodendron:zircon_glass_pane_side_alt" }
    },
    {   "when": { "west": true },
      "apply": { "model": "lepidodendron:zircon_glass_pane_side_alt", "y": 90 }
    },
    {   "when": { "north": false },
      "apply": { "model": "lepidodendron:zircon_glass_pane_noside" }
    },
    {   "when": { "east": false },
      "apply": { "model": "lepidodendron:zircon_glass_pane_noside_alt" }
    },
    {   "when": { "south": false },
      "apply": { "model": "lepidodendron:zircon_glass_pane_noside_alt", "y": 90 }
    },
    {   "when": { "west": false },
      "apply": { "model": "lepidodendron:zircon_glass_pane_noside", "y": 270 }
    },


    {   "when": { "ne": true },
      "apply": { "model": "lepidodendron:zircon_glass_pane_corner_r", "y": 270 }
    },
    {   "when": { "nw": true },
      "apply": { "model": "lepidodendron:zircon_glass_pane_corner_l", "y": 270 }
    },
    {   "when": { "se": true },
      "apply": { "model": "lepidodendron:zircon_glass_pane_corner_l", "y": 90 }
    },
    {   "when": { "sw": true },
      "apply": { "model": "lepidodendron:zircon_glass_pane_corner_r", "y": 90 }
    },
    {   "when": { "es": true },
      "apply": { "model": "lepidodendron:zircon_glass_pane_corner_r" }
    },
    {   "when": { "en": true },
      "apply": { "model": "lepidodendron:zircon_glass_pane_corner_l" }
    },
    {   "when": { "wn": true },
      "apply": { "model": "lepidodendron:zircon_glass_pane_corner_r", "y": 180 }
    },
    {   "when": { "ws": true },
      "apply": { "model": "lepidodendron:zircon_glass_pane_corner_l", "y": 180 }
    }
  ]
}
