{
    "multipart": [
        {   "when": { "layers": 1 },
            "apply": { "model": "lepidodendron:guano_height2" }
        },
        {   "when": { "layers": 2 },
            "apply": { "model": "lepidodendron:guano_height4" }
        },
        {   "when": { "layers": 3 },
            "apply": { "model": "lepidodendron:guano_height6" }
        },
        {   "when": { "layers": 4 },
            "apply": { "model": "lepidodendron:guano_height8" }
        },
        {   "when": { "layers": 5 },
            "apply": { "model": "lepidodendron:guano_height10" }
        },
        {   "when": { "layers": 6 },
            "apply": { "model": "lepidodendron:guano_height12" }
        },
        {   "when": { "layers": 7 },
            "apply": { "model": "lepidodendron:guano_height14" }
        },
        {   "when": { "layers": 8 },
            "apply": { "model": "lepidodendron:guano" }
        },
        {   "when": { "north": true },
            "apply": [
                { "model": "lepidodendron:guano_side_1" },
                { "model": "lepidodendron:guano_side_2" },
                { "model": "lepidodendron:guano_side_3" },
                { "model": "lepidodendron:guano_side_4" }
            ]
        },
        {   "when": { "east": true },
            "apply": [
                { "model": "lepidodendron:guano_side_1", "y": 90 },
                { "model": "lepidodendron:guano_side_2", "y": 90 },
                { "model": "lepidodendron:guano_side_3", "y": 90 },
                { "model": "lepidodendron:guano_side_4", "y": 90 }
            ]
        },
        {   "when": { "south": true },
            "apply": [
                { "model": "lepidodendron:guano_side_1", "y": 180 },
                { "model": "lepidodendron:guano_side_2", "y": 180 },
                { "model": "lepidodendron:guano_side_3", "y": 180 },
                { "model": "lepidodendron:guano_side_4", "y": 180 }
            ]
        },
        {   "when": { "west": true },
            "apply": [
                { "model": "lepidodendron:guano_side_1", "y": 270 },
                { "model": "lepidodendron:guano_side_2", "y": 270 },
                { "model": "lepidodendron:guano_side_3", "y": 270 },
                { "model": "lepidodendron:guano_side_4", "y": 270 }
            ]
        }
    ]
}
