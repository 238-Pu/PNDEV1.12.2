{
    "multipart": [
        {   "apply": { "model": "lepidodendron:gunnera_base" }
        },
        {   "when": { "north": true },
            "apply": { "model": "lepidodendron:gunnera_base_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "lepidodendron:gunnera_base_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "lepidodendron:gunnera_base_side", "y": 180 }
        },
        {   "when": { "west": true },
            "apply": { "model": "lepidodendron:gunnera_base_side", "y": 270 }
        },
        {   "when": { "up": true },
            "apply": { "model": "lepidodendron:gunnera_base_top" }
        }
    ]
}