{
  "multipart": [
    {   "when": { "facing": "south" },
      "apply": { "model": "lepidodendron:nypa_log" }
    },
    {   "when": { "facing": "north" },
      "apply": { "model": "lepidodendron:nypa_log" }
    },
    {   "when": { "facing": "east" },
      "apply": { "model": "lepidodendron:nypa_log", "x": 90 }
    },
    {   "when": { "facing": "west" },
      "apply": { "model": "lepidodendron:nypa_log", "x": 90 }
    },
    {   "when": { "facing": "up" },
      "apply": { "model": "lepidodendron:nypa_log", "x": 90, "y": 90 }
    },
    {   "when": { "facing": "down" },
      "apply": { "model": "lepidodendron:nypa_log", "x": 90, "y": 90 }
    },


    {   "when": { "roots_north": true },
      "apply": { "model": "lepidodendron:nypa_roots" }
    },
    {   "when": { "roots_south": true },
      "apply": { "model": "lepidodendron:nypa_roots", "y": 180 }
    },
    {   "when": { "roots_east": true },
      "apply": { "model": "lepidodendron:nypa_roots", "y": 90 }
    },
    {   "when": { "roots_west": true },
      "apply": { "model": "lepidodendron:nypa_roots", "y": 270 }
    }
  ]
}