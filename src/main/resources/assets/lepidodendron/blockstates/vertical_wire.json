{
  "multipart": [
    {   "when": { "north": false, "east": false, "south": false, "west": false },
      "apply": { "model": "lepidodendron:vertical_wire_post" }
    },
    {   "when": { "north": true },
      "apply": { "model": "lepidodendron:vertical_wire_side" }
    },
    {   "when": { "east": true },
      "apply": { "model": "lepidodendron:vertical_wire_side", "y": 90, "uvlock": true }
    },
    {   "when": { "south": true },
      "apply": { "model": "lepidodendron:vertical_wire_side_alt" }
    },
    {   "when": { "west": true },
      "apply": { "model": "lepidodendron:vertical_wire_side_alt", "y": 90, "uvlock": true }
    }
  ]
}
